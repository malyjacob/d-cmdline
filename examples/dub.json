{
    "name": "examples",
    "dependencies": {
        "cmdline": {
            "path": "../",
            "version": "*"
        }
    },
    "targetPath": "./bin",
    "targetType": "executable",
    "version": "0.0.1",
    "dflags": [
        "-preview=in"
    ],
    "configurations": [
        {
            "name": "all-debug",
            "targetType": "none",
            "preGenerateCommands": [
                "\"$DUB\" build -c deploy",
                "\"$DUB\" build -c str_util",
                "\"$DUB\" build -c calc"
            ]
        },
        {
            "name": "all-release",
            "targetType": "none",
            "preGenerateCommands": [
                "\"$DUB\" build -c deploy -b release",
                "\"$DUB\" build -c str_util -b release",
                "\"$DUB\" build -c calc -b release"
            ]
        },
        {
            "name": "deploy",
            "targetName": "deploy",
            "mainSourceFile": "./deploy.d"
        },
        {
            "name": "str_util",
            "targetName": "str_util",
            "mainSourceFile": "./str_util.d"
        },
        {
            "name": "calc",
            "targetName": "calc",
            "mainSourceFile": "./calc.d"
        }
    ]
}