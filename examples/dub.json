{
    "name": "examples",
    "dependencies": {
        "cmdline": {
            "path": "../",
            "version": "*"
        }
    },
    "targetPath": "./bin",
    "targetType": "executable",
    "version": "0.0.1",
    "dflags": [
        "-preview=in"
    ],
    "configurations": [
        {
            "name": "all-debug",
            "targetType": "none",
            "preGenerateCommands": [
                "\"$DUB\" build -c deploy",
                "\"$DUB\" build -c str_util",
                "\"$DUB\" build -c calc",
                "\"$DUB\" build -c cheese",
                "\"$DUB\" build -c defcmd",
                "\"$DUB\" build -c jread",
                "\"$DUB\" build -c greet",
                "\"$DUB\" build -c variadic",
                "\"$DUB\" build -c defaultval",
                "\"$DUB\" build -c env"
            ]
        },
        {
            "name": "all-release",
            "targetType": "none",
            "preGenerateCommands": [
                "\"$DUB\" build -c deploy -b release",
                "\"$DUB\" build -c str_util -b release",
                "\"$DUB\" build -c calc -b release",
                "\"$DUB\" build -c cheese -b release",
                "\"$DUB\" build -c defcmd -b release",
                "\"$DUB\" build -c jread -b release",
                "\"$DUB\" build -c greet -b release",
                "\"$DUB\" build -c variadic -b release",
                "\"$DUB\" build -c defaultval -b release",
                "\"$DUB\" build -c env -b release"
            ]
        },
        {
            "name": "deploy",
            "targetName": "deploy",
            "mainSourceFile": "./deploy.d"
        },
        {
            "name": "str_util",
            "targetName": "str_util",
            "mainSourceFile": "./str_util.d"
        },
        {
            "name": "calc",
            "targetName": "calc",
            "mainSourceFile": "./calc.d"
        },
        {
            "name": "cheese",
            "targetName": "cheese",
            "mainSourceFile": "./cheese.d"
        },
        {
            "name": "defcmd",
            "targetName": "defcmd",
            "mainSourceFile": "./defcmd.d"
        },
        {
            "name": "jread",
            "targetName": "jread",
            "mainSourceFile": "./jread.d"
        },
        {
            "name": "greet",
            "targetName": "greet",
            "mainSourceFile": "./greet.d"
        },
        {
            "name": "variadic",
            "targetName": "variadic",
            "mainSourceFile": "./variadic.d"
        },
        {
            "name": "defaultval",
            "targetName": "defaultval",
            "mainSourceFile": "./defaultval.d"
        },
        {
            "name": "env",
            "targetName": "env",
            "mainSourceFile": "./env.d"
        }
    ]
}
